{
  "name": "oodzfa-minecraft-server-console",
  "version": "1.2.1",
  "description": "oodzfa minecraft server console",
  "main": "main.js",
  "scripts": {
    "start": "electron ."
  },
  "build": {
    "win": {
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "universal"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "repository": "https://github.com/oodzfa/MinecraftServerConsole",
  "keywords": [
    "Electron",
    "oodzfa",
    "server",
    "console"
  ],
  "author": "oodzfa",
  "license": "MIT",
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.0.3",
    "child_process": "^1.0.2",
    "jquery": "^3.7.1",
    "tailwindcss": "^4.0.3"
  }
}
